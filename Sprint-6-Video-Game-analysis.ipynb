    groups = []
    for rating in main_ratings:
        rating_sales = games_main_ratings[games_main_ratings['rating'] == rating][region]
        groups.append(rating_sales.values)
    
    # Realizar ANOVA
    f_stat, p_value = st.f_oneway(*groups)
    
    print(f"F-estadístico: {f_stat:.4f}")
    print(f"p-valor: {p_value:.6f}")
    
    # Interpretación
    alpha = 0.05
    if p_value < alpha:
        print(f"✓ SIGNIFICATIVO (p < {alpha}): Las clasificaciones ESRB SÍ afectan las ventas")
    else:
        print(f"✗ NO SIGNIFICATIVO (p ≥ {alpha}): Las clasificaciones ESRB NO afectan significativamente las ventas")

